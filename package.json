{
  "name": "semantic-ui-riot",
  "version": "2.1.0",
  "description": "Semantic UI for Riot",
  "main": "dist/semantic-ui-riot.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run compile-watch & npm run test-watch",
    "clean": "rimraf ./coverage && rimraf ./dist/tags",
    "compile": "npm run clean && riot tags --output dist/tags",
    "compile-watch": "npm run clean && riot -w tags --output dist/tags",
    "test": "karma start",
    "test-watch": "karma start --no-single-run",
    "test-run": "karma run",
    "parcel": "parcel build src/index.js --out-dir=dist",
    "build": "npm run compile && npm run parcel"
  },
  "keywords": [
    "riotjs",
    "riot",
    "semantic",
    "semanticui"
  ],
  "author": {
    "name": "black-trooper",
    "url": "https://github.com/black-trooper/semantic-ui-riot/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/black-trooper/semantic-ui-riot.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/black-trooper/semantic-ui-riot/issues"
  },
  "dependencies": {
    "@riotjs/observable": "^4.0.4",
    "date-fns": "^1.29.0",
    "q": "^1.5.1",
    "riot": "4.11.0"
  },
  "devDependencies": {
    "@riotjs/cli": "^4.0.2",
    "@riotjs/compiler": "4.5.2",
    "@riotjs/parcel-plugin-riot": "^4.0.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015-riot": "^1.1.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^3.1.1",
    "htmlhint-riot-rules": "^0.3.14",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-coveralls": "^2.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-parcel": "^0.5.2",
    "karma-riot": "^2.0.0",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^4.0.1",
    "parcel-bundler": "^1.12.4",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.13.0",
    "uuid": "^3.3.3"
  }
}
